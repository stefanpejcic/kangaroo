# Description: Web Server
Host webserver1
    HostName 192.168.1.10
    User youruser
    IdentityFile ~/.ssh/jumpserver_key
    CertificateFile ~/.ssh/jumpserver_key-cert.pub

# Description: Database Server
Host dbserver1
    HostName 192.168.1.20
    User youruser
    IdentityFile ~/.ssh/jumpserver_key
    CertificateFile ~/.ssh/jumpserver_key-cert.pub

# Description: File Server
Host fileserver1
    HostName 192.168.1.30
    User youruser
    IdentityFile ~/.ssh/jumpserver_key
    CertificateFile ~/.ssh/jumpserver_key-cert.pub
